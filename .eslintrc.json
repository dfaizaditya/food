{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "quotes":"off",
        "no-underscore-dangle":"off",
        "object-curly-newline":"off",
        "indent":"off",
        "comma-dangle":"off",
        "operator-linebreak":"off",
        "no-restricted-globals":"off"
    }
}
